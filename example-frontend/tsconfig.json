{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": ["bun-types"],
    "paths": {
      "@/*": ["./*"],
      "@components": ["./components"],
      "@constants": ["./constants"],
      "@assets": ["./assets"],
      "@app": ["./app"],
      "@utils": ["./utils"],
      "@store": ["./store"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
