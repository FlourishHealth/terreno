{
  "dependencies": {
    "@react-navigation/native": "catalog:",
    "@sentry/react": "catalog:",
    "@terreno/rtk": "workspace:*",
    "@terreno/ui": "workspace:*",
    "expo": "catalog:",
    "expo-router": "catalog:",
    "react": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@rtk-query/codegen-openapi": "catalog:",
    "@types/react": "catalog:",
    "bun-types": "^1.3.4",
    "react-test-renderer": "catalog:",
    "typescript": "catalog:"
  },
  "main": "expo-router/entry",
  "name": "@terreno/example-frontend",
  "private": true,
  "scripts": {
    "android": "bun expo start --android --port 8082",
    "export": "expo export -p web",
    "ios": "bun expo start --ios --port 8082",
    "lint": "bun biome check .",
    "lint:fix": "bun biome check --write .",
    "lint:unsafefix": "bun biome check --write --unsafe",
    "sdk": "bun scripts/generate-sdk.ts && bun biome check --write scripts/generate-sdk.ts",
    "start": "bun expo start --port 8082",
    "test": "bun test",
    "test:ci": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "web": "bun expo start --web --port 8082"
  },
  "version": "1.0.0"
}
