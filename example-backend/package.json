    "@thream/socketio-jwt": "^3.1.4",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.0",
    "mongoose": "^8.0.0",
    "passport-local-mongoose": "^9.0.1",
